{% extends 'base.html.twig' %}

{% block title %} Administration des plats{% endblock %}

{% block body %}
    <main class="container">
                <section class="row">
                        <div class="col-12">
                                <h1>Administration des plats</h1>
                        </div>
                        {% for Meal in mealMethode %}
                        <div class="col-12">
                        {{Meal.category.title}}
                        </div>
                        <div class="col-12">
                        {{Meal.title}}
                        </div>
                        {% for image in Meal.Image %}
                        <pre>
                        {{dump(image)}}
                        </pre>
                        {% endfor %}
                </section>
        </main>
 {% endfor %}
{% endblock %}